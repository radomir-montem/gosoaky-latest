              </div>
              <div
                data-w-id="b03cd8dd-1cef-5c0b-24bb-b3ab4adb648d"
                class="go-anatomy-grid-block-line-right-aligned"
                style="will-change: width, height; width: 100%;"
              ></div>
            {% endif %}
          </div>
          {% comment %} RIGHT 5 {% endcomment %}
          <div id="w-node-_339cba83-45fc-4894-4e44-f80a3979d033-1658e309" class="go-anatomy-grid-block-right">
            {% comment %}
              cycling features: true // false as Bool
              namespace.key:        product_information.cycling_features
            {% endcomment %}
            {% if anatomy_product.metafields.product_information.cycling_features == true %}
              {% render 'feature-icons', icon:"cycling-features", color:color%}
            <div
              data-w-id="339cba83-45fc-4894-4e44-f80a3979d035"
              class="go-anatomy-feature-title-right"
              style="will-change: opacity; opacity: 1;"
            >
              CYCLING FEATURES
            </div>
            <div
              data-w-id="339cba83-45fc-4894-4e44-f80a3979d037"
              class="go-anatomy-grid-block-line-left-aligned"
              style="will-change: width, height; width: 100%;"
            ></div>
            {%endif%}
          </div>
        </div>
        <div class="go-anatomy-button-block">
          <div class="go-hero-div-height">
            <a data-w-id="565f41e2-3cb7-f548-0dc3-fb6ff2cb5118" href="{{ icon_guide_url }}" class="full-unstyled-link button-with--icon" target="_blank">
              icon guide
              {% render 'icon-button' %}
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
