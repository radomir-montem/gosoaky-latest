              </div>
              <div
                data-w-id="63597cf4-016f-f303-f629-eca3ae0ad381"
                class="go-anatomy-grid-block-line-left-aligned"
                style="will-change: width, height; width: 100%;"
              ></div>
            {% endif %}
          </div>
          {% comment %} LEFT 4 {% endcomment %}
          <div
            id="w-node-_39dd8189-8bbb-84fd-ad87-691b43114ba3-1658e309"
            data-w-id="39dd8189-8bbb-84fd-ad87-691b43114ba3"
            class="go-anatomy-grid-block-left"
          >
            {% comment %}
              Reflective visibility strip: true // false as Bool
              namespace.key:        product_information.reflective_visibility_strip
            {% endcomment %}
            {% if anatomy_product.metafields.product_information.reflective_visibility_strip == true %}
              {% render 'feature-icons', icon:"reflective-visibility-strip", color:color%}
              <div
                data-w-id="39dd8189-8bbb-84fd-ad87-691b43114ba5"
                class="go-anatomy-feature-title"
                style="will-change: opacity; opacity: 1;"
              >
                REFLECTIVE VISIBILITY STRIP
              </div>
              <div
                data-w-id="39dd8189-8bbb-84fd-ad87-691b43114ba7"
                class="go-anatomy-grid-block-line-right-aligned"
                style="will-change: width, height; width: 100%;"
              ></div>
            {% endif %}
          </div>
          {% comment %} RIGHT 4 {% endcomment %}
          <div id="w-node-f4931144-a5c0-b068-435d-84990f13cca9-1658e309" class="go-anatomy-grid-block-right">
            {% comment %}
              Windproof: true // false as Bool
              namespace.key:        product_information.windproof
            {% endcomment %}
            {% if anatomy_product.metafields.product_information.windproof == true %}
              {% render 'feature-icons', icon:"windproof", color:color%}
              <div
                data-w-id="f4931144-a5c0-b068-435d-84990f13ccab"
                class="go-anatomy-feature-title-right"
                style="will-change: opacity; opacity: 1;"
              >
                WINDPROOF
              </div>
              <div
                data-w-id="f4931144-a5c0-b068-435d-84990f13ccad"
                class="go-anatomy-grid-block-line-left-aligned"
                style="will-change: width, height; width: 100%;"
              ></div>
            {% endif %}
          </div>
          {% comment %} LEFT 5 {% endcomment %}
          <div
            id="w-node-b03cd8dd-1cef-5c0b-24bb-b3ab4adb6489-1658e309"
            data-w-id="b03cd8dd-1cef-5c0b-24bb-b3ab4adb6489"
            class="go-anatomy-grid-block-left"
          >
            {% comment %}
              Dupont sorona biodegradable insulation: true // false as Bool
              namespace.key:        product_information.bio_insulation
            {% endcomment %}
            {% if anatomy_product.metafields.product_information.bio_insulation == true %}
              {% render 'feature-icons', icon:"bio-isolation", color:color%}
              <div
                data-w-id="b03cd8dd-1cef-5c0b-24bb-b3ab4adb648b"
                class="go-anatomy-feature-title"
                style="will-change: opacity; opacity: 1;"
              >
